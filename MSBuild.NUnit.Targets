<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <MSBuildNUnitTaskPath Condition="'$(MSBuildNUnitTaskPath)' == ''">$(MSBuildExtensionsPath)\MSBuildNUnitTask</MSBuildNUnitTaskPath>
    <MSBuildNUnitTaskLib>$(MSBuildNUnitTaskPath)\MSBuild.NUnit.dll</MSBuildNUnitTaskLib>
  </PropertyGroup>
  <UsingTask AssemblyFile="$(MSBuildNUnitTaskLib)" TaskName="MSBuild.Tasks.NUnit" />
</Project>
