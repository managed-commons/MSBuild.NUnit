<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>MSBuild.NUnit</id>
        <version>1.0.0.0</version>
		<title>MSBuild NUnit Task</title>
        <description>MSBuild.NUnit is a MSBuild task to integrate the NUnit testing framework, to support Test Driven Development.</description>
        <authors>
            <author>MSBuild NUnit Task Project</author>
            <author>Paul Welter</author>
        </authors>
 		<licenseUrl>https://github.com/monoman/MSBuild.NUnit/blob/master/README.markdown</licenseUrl>
		<projectUrl>https://github.com/monoman/MSBuild.NUnit</projectUrl>
       <language>en-US</language>
		<dependencies>
			<dependency id="NUnit" minVersion="2.5.8.10295" />
		</dependencies>
    </metadata>
	<files>
		  <file src="bin\Debug\*.dll" target="Tools\MSBuild.Nunit" /> 
		  <file src="bin\Debug\*.pdb" target="Tools\MSBuild.Nunit" /> 
		  <file src="NUnit\bin\net-2.0\**\*.*" target="Tools\MSBuild.Nunit" />
	</files>
</package>

